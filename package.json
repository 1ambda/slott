    "react-dom": "0.14.7",
    "react-redux": "4.4.0",
    "react-router": "2.0.0",
    "react-router-redux": "4.0.0",
    "react-tap-event-plugin": "0.2.1",
    "redux": "3.3.1",
    "redux-saga": "0.9.5"
  },
  "devDependencies": {
    "autoprefixer": "6.3.3",
